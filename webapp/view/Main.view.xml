<mvc:View xmlns:layout="sap.ui.layout"
    controllerName="fr.mca.importlib.controller.Main"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ndc="sap.ndc"
    xmlns="sap.m"
    xmlns:grid="sap.ui.layout.cssgrid"
    xmlns:f="sap.f"
    height="100%">
    <Page id="MainContentPage"
        showHeader="false"
        title="{i18n>Main.Title}">
        <content>
            <MessageStrip id="done"
                text="Loaded"/>
        </content>
        <footer>
            <OverflowToolbar id="MainFooter">
                <ToolbarSpacer id="MainFooterSpacer1"/>
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>